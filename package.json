{
	"name": "pothos-plugin-result",
	"description": "A Pothos plugin for easily creating GraphQL schemas that reflect the results of specific actions.",
	"publishConfig": {
		"access": "public",
		"registiry": "https://registry.npmjs.org"
	},
	"author": {
		"name": "Chanhee Lee",
		"email": "contact@imch.dev"
	},
	"license": "MIT",
	"version": "1.0.2",
	"files": [
		"package.json",
		"README.md",
		"dist"
	],
	"type": "module",
	"main": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.mjs",
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"test": "vitest",
		"build": "nanobundle build",
		"ci": "yarn test --coverage && yarn build",
		"prepack": "yarn build",
		"publish": "yarn npm publish"
	},
	"keywords": [
		"pothos",
		"plugin",
		"pothos-graphql",
		"graphql"
	],
	"packageManager": "yarn@4.2.2",
	"peerDependencies": {
		"@pothos/core": "^3"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.0",
		"@pothos/core": "^3.41.1",
		"@pothos/plugin-with-input": "^3.10.1",
		"@total-typescript/tsconfig": "^1.0.4",
		"graphql": "^16.8.1",
		"nanobundle": "^2.1.0",
		"typescript": "beta",
		"vitest": "^1.6.0"
	}
}
